{#
/**
 * @file subject_drupal.html.twig
 * Default theme implementation to present Subject drupal data.
 *
 * This template is used when viewing Subject drupal pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_subject_drupal()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('subject_drupal') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
